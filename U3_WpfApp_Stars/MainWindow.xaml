<Window x:Class="U3_WpfApp_Stars.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:U3_WpfApp_Stars"
        mc:Ignorable="d"
        Title="Stars" Height="330" Width="330">
    <Grid>
        <Canvas Name="mainCanvas">
            <!-- Polygon with points that make up a star -->
            <Polygon Name="star" Fill="Green" Points="205,150 217,186 259,186 
         223,204 233,246 205,222 177,246 187,204 151,186 193,186" />
        </Canvas>
    </Grid>
</Window>
